<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Session_Date_must_be_After_Today</fullName>
    <active>true</active>
    <errorConditionFormula>Session_Date__c &lt;= TODAY()</errorConditionFormula>
    <errorMessage>Session date cannot be in the past.</errorMessage>
</ValidationRule>
